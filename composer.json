{
  "name": "passionweb/extending-extbase-model",
  "license": "GPL-3.0",
  "description": "Shows example of extending an Extbase model (add author image to EXT:news article). (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^12.4 | ^12.4",
    "typo3/cms-extbase": "^12.4 | ^12.4",
    "georgringer/news": "^11.4"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\ExtendingExtbaseModel\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/extending-extbase-model": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "extending_extbase_model"
    }
  }
}
